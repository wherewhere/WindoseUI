            <!-- Brushes -->
            <SolidColorBrush x:Key="WindoseTextFillColorPrimaryBrush" Color="{StaticResource WindoseTextFillColorPrimary}" />
            <SolidColorBrush x:Key="WindoseTextFillColorSecondaryBrush" Color="{StaticResource WindoseTextFillColorSecondary}" />
            <SolidColorBrush x:Key="WindoseTextFillColorTertiaryBrush" Color="{StaticResource WindoseTextFillColorTertiary}" />
            <SolidColorBrush x:Key="WindoseTextFillColorDisabledBrush" Color="{StaticResource WindoseTextFillColorDisabled}" />
            <SolidColorBrush x:Key="WindoseTextFillColorInverseBrush" Color="{StaticResource WindoseTextFillColorInverse}" />
            
            <SolidColorBrush x:Key="WindoseControlFillColorDefaultBrush" Color="{StaticResource WindoseControlFillColorDefault}" />
            <SolidColorBrush x:Key="WindoseControlFillColorDisabledBrush" Color="{StaticResource WindoseControlFillColorDisabled}" />

            <SolidColorBrush x:Key="WindoseControlTopLeftStrokeColorDefaultBrush" Color="{StaticResource WindoseControlTopLeftStrokeColorDefault}" />
            <SolidColorBrush x:Key="WindoseControlTopLeftStrokeColorOnAccentDefaultBrush" Color="{StaticResource WindoseControlTopLeftStrokeColorOnAccentDefault}" />

            <SolidColorBrush x:Key="WindoseControlBottomRightStrokeColorDefaultBrush" Color="{StaticResource WindoseControlBottomRightStrokeColorDefault}" />
            <SolidColorBrush x:Key="WindoseControlBottomRightStrokeColorOnAccentDefaultBrush" Color="{StaticResource WindoseControlBottomRightStrokeColorOnAccentDefault}" />

            <SolidColorBrush x:Key="WindoseAccentFillColorDefaultBrush" Color="{ThemeResource WindoseSystemAccentColor}" />

            <!-- Common -->
            <SolidColorBrush x:Key="SystemControlTransparentBrush" Color="Transparent" />
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
    <FontFamily x:Key="WindoseContentControlThemeFontFamily">DinkieBitmap 9px,XamlAutoFontFamily</FontFamily>
</ResourceDictionary>
